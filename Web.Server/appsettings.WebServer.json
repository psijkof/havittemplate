{
	"ConnectionStrings": {
		"Database": "{PLACEHOLDER}",
		"AzureStorage": ""
	},
	"AzureAd": {
		"Instance": "https://login.microsoftonline.com",
		"TenantId": "{DEPLOYMENT_SPECIFIC}",
		"ClientId": "{DEPLOYMENT_SPECIFIC}",
		"ClientSecret": "{DEPLOYMENT_SPECIFIC}"
	},
	"ApplicationInsights": {
		"ConnectionString": "{DEPLOYMENT_SPECIFIC}",
		"DeveloperMode": false
	},
	"AppSettings": {
		"ApplicationFileStorage": {
			"PathOrContainerName": "{DEPLOYMENT_SPECIFIC}"
		},
		"SmtpExceptionMonitoring": {
			"Enabled": true,
			"Subject": "NewProjectTemplate (Web.Server)",
			"From": "errors@mydomain.com",
			"To": "errors@mydomain.com",
			"SmtpServer": "errorssmtp.server.com"
		},
		"MailingOptions": {
			"SmtpServer": "{DEPLOYMENT_SPECIFIC}",
			"From": "{DEPLOYMENT_SPECIFIC}",
			"SmtpUsername": "",
			"SmtpPassword": "",
			"SmtpPort": 25,
			"UseSsl": false
		},
		"Migrations": {
			"CommandTimeout": 300,
			"RunMigrations": false
		}
	},
	"WebClient": {
		"ApplicationInsights": {
			"ConnectionString": null
		}
	},
	"Logging": {
		"IncludeScopes": false,
		"LogLevel": {
			"Default": "Warning",
			"Microsoft.Hosting.Lifetime": "Information"
		}
	},
	"AllowedHosts": "*"
}
